   "outputs": [],
   "source": [
    "def make_uppercase(func):\n",
    "    def wrapper(*args):\n",
    "        org_op = func(*args)\n",
    "        return org_op.upper()\n",
    "    return wrapper"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 9,
   "id": "138455e3-845c-4f46-82ce-bf427b7efb15",
   "metadata": {},
   "outputs": [],
   "source": [
    "@make_uppercase\n",
    "def greet(say):\n",
    "    return say +'!...'"
   ]
  },
